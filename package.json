{
  "name": "superagent-get-chunked",
  "version": "1.0.0-alpha.0",
  "description": "Adds support to the superagent library for chunked-transfer encoded responses when in the browser.",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && npm run compress",
    "compile": "npm-run-all compile:*",
    "compile:main": "node ./build/mask-maker.js && browserify index.js --debug -o ./test/browser/superagent-get-chunked.js && echo 'onmessage = function(e) {' > ./test/browser/superagent-get-chunked-worker.js && browserify ./lib/superagent-worker-body.js >> ./test/browser/superagent-get-chunked-worker.js && echo '};' >> ./test/browser/superagent-get-chunked-worker.js && cp ./test/browser/superagent-get-chunked-worker.js ./test/browser/superagent-get-chunked-worker.min.js && node ./build/mask-maker-test.js",
    "compile:c3": "browserify ./test/browser/c3.js --standalone c3 -o ./test/browser/c3.bundle.js",
    "compile:application_json": "browserify ./lib/parsers/application_json.js --standalone application_json -o ./test/browser/application_json.js",
    "compile:application_octet_stream": "browserify ./lib/parsers/application_octet_stream.js --standalone application_octet_stream -o ./test/browser/application_octet_stream.js",
    "compile:application_x_ndjson": "browserify ./lib/parsers/application_x_ndjson.js --standalone application_x_ndjson -o ./test/browser/application_x_ndjson.js",
    "compile.broken:application_xml": "browserify ./lib/parsers/application_xml.js --standalone application_xml -o ./test/browser/application_xml.js",
    "compile:text_csv": "browserify ./lib/parsers/text_csv.js --standalone text_csv -o ./test/browser/text_csv.js",
    "compile:text_n3": "browserify ./lib/parsers/text_n3.js --standalone text_n3 -o ./test/browser/text_n3.js",
    "compile:text_tab_separated_values": "browserify ./lib/parsers/text_tab_separated_values.js --standalone text_tab_separated_values -o ./test/browser/text_tab_separated_values.js",
    "compress": "uglifyjs ./test/browser/superagent-get-chunked-worker.js -c --debug -o ./test/browser/superagent-get-chunked-worker.min.js",
    "size": "browserify -g uglifyify ./size.js --standalone size | uglifyjs -c -o ./size.bundle.js && ls -lh | grep 'size.bundle.js' && rm ./size.bundle.js",
    "start": "echo 'Once build finishes, visit http://localhost:8080' && npm run build && cd ./test/browser && http-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ariutta/superagent-get-chunked.git"
  },
  "keywords": [
    "request",
    "superagent"
  ],
  "author": "Anders Riutta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ariutta/superagent-get-chunked/issues"
  },
  "homepage": "https://github.com/ariutta/superagent-get-chunked#readme",
  "dependencies": {
    "JSONStream": "^1.1.3",
    "base58": "^0.1.0",
    "bitmask": "young-steveo/bitmask.js#345d7088bc2f52a87a0e02c2a89b5f40f03c1d9d",
    "component-emitter": "^1.2.1",
    "event-emitter": "^0.3.4",
    "eventemitter3": "^1.2.0",
    "install": "^0.8.1",
    "js-csvparser": "^1.4.0",
    "lodash": "^4.13.1",
    "n3": "^0.5.0",
    "ndjson": "^1.4.3",
    "npm": "^3.10.5",
    "npm-run-all": "^2.3.0",
    "requests": "ariutta/requests#49d2a241e316ac28f30dc57c73d1f96b3a7cfcef",
    "rx-extra": "^1.0.2",
    "sax": "^1.2.1",
    "superagent": "^2.1.0",
    "ys-hash": "^0.1.2"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "browserify": "^13.0.1",
    "c3": "^0.4.11",
    "component-emitter": "^1.2.1",
    "http-server": "^0.9.0",
    "uglify-js": "^2.7.0",
    "uglifyify": "^3.0.2"
  }
}
